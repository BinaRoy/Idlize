{
    "compilerOptions": {
        "target": "es2017",
        "lib": ["ESNext", "ESNext.WeakRef"],
        "moduleResolution": "node",
        "composite": true,
        "incremental": true,
        "declarationMap": true,
        "sourceMap": true,
        "declaration": true,
        "noEmitOnError": true,
        "strict": true,
        "skipLibCheck": true,
        "removeComments": false,
        "outDir": "build",
        "rootDir": "generated",
        "module": "CommonJS",
        "paths": {
            "@arkoala/arkui": ["./generated/ts", "./"],
            "@arkoala/arkui/*": ["./generated/ts/*", "./*"],
            "@koalaui/arkoala": ["./generated/ts", "./"],
        },
    },
    "include": [
        "generated/sig/arkoala/**/*",
        "generated/sig/arkoala-arkts/framework/**/*"
    ],
    "files": [
        "generated/ts/index.d.ts"
    ]
}
