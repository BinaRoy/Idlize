{
    "compilerOptions": {
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "node",
        "outDir": "build/ts",
        "sourceMap": true,
        "baseUrl": ".",
        "paths": {
            "#compat": ["./src/node/compat.ts"],
            "#handwritten": ["./src/node/handwritten/"],
            "#external_lib": ["./src/node/handwritten/external_lib.ts"]
        }
    },
    "include": ["src/*.ts", "src/node/**/*.ts", "generated/ts/**/*.ts"],
}
