{
    "compilerOptions": {
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "node",
        "outDir": "build/ts",
        "sourceMap": true,
        "baseUrl": ".",
        "paths": {
            "#compat": ["./src/node/compat.ts"],
            "#handwritten": ["./src/node/handwritten/"],
            "@internal.lib": ["./src/node/modules/internal_lib"],
            "@external.lib": ["./src/node/handwritten/external_lib.ts"],
            "@external.lib.sdk": ["./src/node/handwritten/external_lib_sdk.ts"]
        }
    },
    "include": ["src/*.ts", "src/node/**/*.ts", "generated/ts/**/*.ts"],
}
