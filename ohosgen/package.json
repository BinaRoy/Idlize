{
  "name": "@idlizer/ohosgen",
  "version": "2.1.7+devel",
  "description": "",
  "main": "lib/index.js",
  "bin": "lib/index.js",
  "files": [
    "lib/*.js",
    "whitelist.json",
    "generation-config/**/*"
  ],
  "license": "Apache-2.0",
  "keywords": [],
  "dependencies": {
    "@idlizer/core": "2.1.7+devel",
    "@idlizer/libohos": "2.1.7+devel",
    "@types/node": "^18.0.0",
    "commander": "^10.0.0",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.0",
    "@types/node": "^18.0.0",
    "source-map-support": "^0.5.21"
  },
  "scripts": {
    "compile": "npm run --prefix .. download:sdk && npm run --prefix ../dtsgen compile && npm run --prefix ../libohos compile && rollup -c",
    "panda:sdk:install": "npm run panda:sdk:install --prefix ../external/incremental/tools/panda",
    "panda:sdk:check-install": "npm run panda:sdk:check-install --prefix ../external/incremental/tools/panda",
    "generate:xml:idl": "npm run compile && node ../dtsgen --dts2idl --output-dir ./demos/xml/generated/idl --base-dir ../sdk-patched/api,./demos/xml --input-files ../sdk-patched/api/@ohos.xml.d.ts ./demos/xml/@ohos.xml.test.d.ts --options-file ./demos/xml/config.json",
    "run:xml:ts": "npm run generate:xml:idl && node . --use-new-ohos --idl2peer --input-dir ./demos/xml/generated/idl --output-dir ./demos/xml --options-file ./demos/xml/config.json",
    "run:xml:arkts": "npm run generate:xml:idl && node . --use-new-ohos --idl2peer --input-dir ./demos/xml/generated/idl --output-dir ./demos/xml --language=arkts --options-file ./demos/xml/config.json",
    "run:xml:cj": "npm run generate:xml:idl && node . --use-new-ohos --idl2peer --input-dir ./demos/xml/generated/idl --output-dir ./demos/xml --language=cangjie --options-file ./demos/xml/config.json",
    "generate:mediaquery:idl": "npm run compile && node ../dtsgen --dts2idl --base-dir ./demos/mediaquery/dts/api --input-files ./demos/mediaquery/dts/api/@ohos.mediaquery.d.ts --output-dir ./demos/mediaquery/generated/idl",
    "run:mediaquery:ts": "npm run generate:mediaquery:idl && node . --use-new-ohos --idl2peer --language=ts --input-dir ./demos/mediaquery/generated/idl --output-dir ./demos/mediaquery --options-file ./demos/mediaquery/config.json",
    "run:mediaquery:arkts": "npm run generate:mediaquery:idl && node . --use-new-ohos --idl2peer --language=arkts --input-dir ./demos/mediaquery/generated/idl --output-dir ./demos/mediaquery --options-file ./demos/mediaquery/config.json",
    "run:mediaquery:cj": "npm run generate:mediaquery:idl && node . --use-new-ohos --idl2peer --language=cangjie --input-dir ./demos/mediaquery/generated/idl --output-dir ./demos/mediaquery --options-file ./demos/mediaquery/config.json",
    "generate:context:idl": "npm run compile && node ../dtsgen --dts2idl --base-dir ./demos/context/dts/api --input-dir ./demos/context/dts/api/application,./demos/context/dts/api/bundleManager --output-dir ./demos/context/generated/idl",
    "run:context:ts": "npm run generate:context:idl && node . --idl2peer --language=ts --input-dir ./demos/context/generated/idl --output-dir ./demos/context --options-file ./demos/context/config.json",
    "run:context:arkts": "npm run generate:context:idl && node . --idl2peer --language=arkts --input-dir ./demos/context/generated/idl --output-dir ./demos/context --options-file ./demos/context/config.json",
    "run:huks:ts": "npm run compile && node . --use-new-ohos --dts2peer --input-files ../interface_sdk-js/api/@ohos.security.huks.d.ts --output-dir ./out/huks --options-file ./demos/huks/config.json",
    "run:huks:arkts": "npm run compile && node . --use-new-ohos --dts2peer --input-files ../interface_sdk-js/api/@ohos.security.huks.d.ts --output-dir ./out/huks --language=arkts --options-file ./demos/huks/config.json",
    "generate:unit:idl": "npm run compile && node ../dtsgen --dts2idl --input-dir ./tests/unit/dts --output-dir ./tests/unit/generated/idl --options-file ./tests/unit/conf/config.json",
    "run:unit:ts": "npm run generate:unit:idl && node . --idl2peer --language=ts --default-idl-package unit --input-dir ./tests/unit/generated/idl,./tests/unit/idl --output-dir ./tests/unit",
    "run:unit:arkts": "npm run generate:unit:idl && node . --idl2peer --language=arkts --default-idl-package unit --input-dir ./tests/unit/generated/idl,./tests/unit/idl --output-dir ./tests/unit",
    "generate:dummy:idl": "npm run compile && node ../dtsgen --dts2idl --input-dir ./tests/dummy/dts --output-dir ./tests/dummy/generated/idl --options-file ./tests/dummy/conf/config.json",
    "run:dummy:ts": "npm run generate:dummy:idl && node . --idl2peer --language=ts --default-idl-package dummy --input-dir ./tests/dummy/generated/idl,./tests/dummy/idl --output-dir ./tests/dummy",
    "run:dummy:arkts": "npm run generate:dummy:idl && node . --idl2peer --language=arkts --default-idl-package dummy --input-dir ./tests/dummy/generated/idl,./tests/dummy/idl --output-dir ./tests/dummy",
    "configure:native-node-host-ts-ohos-context": "cd ./native && meson setup -Dsource_set=ohos-context build-node-host-ts-ohos-context",
    "compile:native-node-host-ts-ohos-context": "npm run configure:native-node-host-ts-ohos-context && cd native && meson compile --verbose -C build-node-host-ts-ohos-context && meson install -C build-node-host-ts-ohos-context",
    "generate-schema": "npm run compile && node . --show-config-schema > generation-config/schema.json"
  }
}